<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app    
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"    
"http://java.sun.com/dtd/web-app_2_3.dtd">  
<web-app>
  
  <servlet>
    <servlet-name>RedisServlet</servlet-name>
    <servlet-class>com.gradven.redisclient.servlet.RedisServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RedisServlet</servlet-name>
    <url-pattern>/servlet/redisservlet</url-pattern>
  </servlet-mapping>

<session-config>
<session-timeout>30</session-timeout> 
</session-config>

</web-app>
